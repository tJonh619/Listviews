<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/btn15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Combinando correspondencia con Word y una base de datos en Excel | Gestión de la Información #15" />

        <Button
            android:id="@+id/btn16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Formato condicional en Excel Parte 1 | Gestión de la Información #16" />

        <Button
            android:id="@+id/btn17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Formato condicional en Excel Parte 2 | Gestión de la Información #17" />

        <Button
            android:id="@+id/btn18"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Formato condicional en Excel Parte 3 | Gestión de la Información #18" />

        <Button
            android:id="@+id/btn19"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="¿Cómo generar minigráficos en Excel? | Gestión de la Información #19" />

        <Button
            android:id="@+id/btn20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Cinta y barra de menú rápida en Excel | Gestión de la Información #20" />

        <Button
            android:id="@+id/btn21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Conoce todos los menús en la presentación de excel | Gestión de la Información #21" />

        <Button
            android:id="@+id/btn22"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Conoce todas las opciones de menú en Excel | Gestión de la Información #22" />

        <Button
            android:id="@+id/btn23"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="¿Cómo hacer consultas web desde Excel y generar Bases de Datos? | Gestión de la Información #23" />

        <Button
            android:id="@+id/btn24"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Creación de Macros en Excel | Gestión de la Información #24" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>